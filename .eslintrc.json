{
  "parser": "babel-eslint",
  "extends": ["airbnb",  
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": ["error", "windows"],
    "import/extensions": "off",
    "import/no-unresolved": [
      2, 
      { "caseSensitive": false }
   ]
  }
}
